{
  "name": "netskope-collector-dev",
  "dockerComposeFile": ["docker-compose.dev.yml"],
  "service": "dev",
  "workspaceFolder": "/workspace",
  "forwardPorts": [4566, 5000],
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python"
      },
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}
